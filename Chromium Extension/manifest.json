{
    "manifest_version": 3,
    "name": "Youtube Anti-SI",
    "version": "1.1",
    "description": "Automatically removes SI parameter from share urls.",
    "icons": {
        "16": "icons/icon-16.png",
        "32": "icons/icon-32.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    },
    "content_scripts": [
        {
            "js": [
                "content.js"
            ],
            "matches": [
                "*://*.youtube.com/*"
            ]
        }
    ]
}